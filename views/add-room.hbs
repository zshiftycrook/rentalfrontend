            <div class="container">
                <div class="row register-form">
                    <div class="col-md-8 col-xl-12 offset-md-2 offset-xl-0">
                        <form class="custom-form" method="POST" action="/auth/registerRoom">
                            <h1>Room Register Form</h1>
                               {{#if message}}
                                <div class="alert">
                                <span class="closebtn">&times;</span>  
                                <strong>Alert!</strong> {{message}}
                                </div>
                                {{/if}}
                                {{#if success}}
                                <div class="alert success">
                                <span class="closebtn">&times;</span>  
                                <strong>Success!</strong> {{success}}
                                </div>       
                                {{/if}}
                            <div class="form-row form-group">
                                <div class="col-sm-4 label-column"><label class="col-form-label" for="name-input-field">Room Number</label></div>
                                <div class="col-sm-6 input-column"><input class="form-control" type="text" required name="roomnumber" autocomplete="off"></div>
                            </div>
                            <div class="form-row form-group">
                                <div class="col-sm-4 label-column"><label class="col-form-label" for="name-input-field">Room Floor</label></div>
                                <div class="col-sm-6 input-column"><input class="form-control" type="text" required name="floor" autocomplete="off"></div>
                            </div>
                            <div class="form-row form-group">
                                <div class="col-sm-4 label-column"><label class="col-form-label" for="name-input-field">Room Size</label></div>
                                <div class="col-sm-6 input-column"><input class="form-control" type="number" name="size" autocomplete="off"></div>
                            </div>
                            <div class="form-row form-group">
                                <div class="col-sm-4 label-column"><label class="col-form-label" for="dropdown-input-field">Status&nbsp;</label></div>
                                <div class="col-sm-4 input-column">
                                    <div class="dropdown">
                                        <select name="Status" id="Status" class="btn btn-primary dropdown-toggle" ><button class="btn btn-primary dropdown-toggle">
                                        <option value="Rented">Rented &nbsp;</option>
                                        <option value="Maintenance">Maintainance</option>
                                        <option value="Available" selected>Available</option>
                                        </select>                                        
                                </div>
                            </div><button class="btn btn-light submit-button" type="submit">Submit Room</button></form>
                    </div>
                </div>
